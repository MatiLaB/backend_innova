{
  "name": "backend_innova",
  "version": "1.0.0",
  "description": "Backend Prototipo Innova",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "dev": "nodemon src/index.js",
    "deploy": "node start.js",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:reset": "run-s db:drop db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatiLaB/backend_innova.git"
  },
  "author": "MatiLaB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MatiLaB/backend_innova/issues"
  },
  "homepage": "https://github.com/MatiLaB/backend_innova#readme",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^3.0.0",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-router": "^13.0.1",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.2"
  }
}
